async function startProgram() {
	await scrollMatrixText('Start', {r: 0, g:200, b: 0}, 6, true)
	await delay(3)
	await roll(0, 100, 1.5)
	setMainLed({r:0, g: 0, b: 255})
	await delay(2)
	await roll(90, 100, 1)
	await delay(4)
	await Sound.play(true)
	await delay(4)
	await roll(180, 100, .7)
}

